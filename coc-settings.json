{
	"languageserver": {
      	"intelephense": {
         	"command": "intelephense",
         	"args": ["--stdio"],
         	"filetypes": ["php"],
         	"initializationOptions": {
                "storagePath": "/tmp/intelephense"
            }
        }, 
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true
            }
        }
    }
}
